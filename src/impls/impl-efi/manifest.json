{
    "$schema": "https://schemas.cute.engineering/stable/cutekit.manifest.component.v1",
    "id": "impl-efi",
    "type": "lib",
    "description": "Karm platform embedding layer for EFI.",
    "enableIf": {
        "sys": [
            "efi"
        ]
    },
    "requires": [
        "efi",
        "abi",
        "karm-core"
    ],
    "provides": [
        "karm-logger-impl",
        "karm-sys-impl",
        "karm-sys-async-impl",
        "karm-sys-sandbox-impl",
        "karm-crypto-impl",
        "karm-ui-impl",
        "karm-app-impl"
    ]
}
