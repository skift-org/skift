{
    "$schema": "https://schemas.cute.engineering/stable/cutekit.manifest.component.v1",
    "id": "impl-skift",
    "type": "lib",
    "description": "Karm platform embedding layer for skift.",
    "enableIf": {
        "sys": [
            "skift"
        ]
    },
    "requires": [
        "crts",
        "abi",
        "hjert-api"
    ],
    "provides": [
        "karm-logger-impl",
        "karm-app-impl",
        "karm-sys-impl",
        "karm-sys-async-impl",
        "karm-sys-sandbox-impl"
    ]
}
